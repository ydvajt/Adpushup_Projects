{
    "manifest_version":2,
    "name": "Content script",
    "version":"1.0",

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],

            "js": ["content.js"]
        }
    ],

    "background": {
        "scripts" : [ "background.js"]
    },
    
    "browser_action" : {
        "default_icons" : "icon_16x16.png",
         "default_popup" : "browser.html"
        
    }
      
      
}

